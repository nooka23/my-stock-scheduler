<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.myunghoon.my-stock-scheduler.daily-stock</string>

  <key>ProgramArguments</key>
  <array>
    <string>/Users/myunghoon/my-stock-scheduler/scripts/run_daily_stock_local.sh</string>
  </array>

  <key>WorkingDirectory</key>
  <string>/Users/myunghoon/my-stock-scheduler</string>

  <key>StartCalendarInterval</key>
  <dict>
    <key>Hour</key>
    <integer>15</integer>
    <key>Minute</key>
    <integer>30</integer>
  </dict>

  <key>StandardOutPath</key>
  <string>/Users/myunghoon/Library/Logs/my-stock-scheduler-daily-stock.out.log</string>

  <key>StandardErrorPath</key>
  <string>/Users/myunghoon/Library/Logs/my-stock-scheduler-daily-stock.err.log</string>

  <key>RunAtLoad</key>
  <false/>
</dict>
</plist>
